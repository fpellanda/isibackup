/mnt/win/CD_ROM
/mnt/win/CD-ROM
/mnt/win/CDR
/mnt/win/CD_R
/mnt/win/CD-R
/mnt/win/data.2nd
/mnt/win/Data.2nd
/mnt/win/TWIXTEL
/mnt/win/Recycled
/mnt/win/backup
/mnt/win/Backup
/mnt/win/support
/mnt/win/Support
/mnt/win/swcoll
/mnt/win/temp
/mnt/win/Temp
/mnt/win/tmp
/mnt/win/Tmp
/mnt/win/TMP
/mnt/win/Windows/temp
/mnt/win/Windows/temp
/mnt/win/windows/Temp
/mnt/win/windows/temp
